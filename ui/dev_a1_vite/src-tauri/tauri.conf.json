{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Gumgang UI",
  "identifier": "com.gumgang.ui",
  "version": "0.1.0",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
